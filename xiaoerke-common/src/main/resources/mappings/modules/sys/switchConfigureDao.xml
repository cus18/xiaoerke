<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxqm.xiaoerke.modules.sys.dao.SwitchConfigureDao">
    
	<select id="getUmbrellaSwitch" resultType="com.cxqm.xiaoerke.modules.sys.entity.SwitchConfigure" parameterType="java.util.Map">
		SELECT 
			* 
		FROM switch_configure
		WHERE 1=1
		<if test="type != null and type != ''">
			AND type = #{type}
		</if>
	</select>
	
</mapper>