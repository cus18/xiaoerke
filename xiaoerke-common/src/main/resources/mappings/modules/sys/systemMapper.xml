<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cxqm.xiaoerke.modules.sys.dao.SystemDao">
	 
    <select id="getWechatParameter" resultType="java.util.HashMap">
        SELECT sw.token, sw.ticket FROM  sys_wechatparameter sw WHERE  sw.id = '1'
    </select>

    <select id="getDoctorWechatParameter" resultType="java.util.HashMap">
        SELECT sw.token, sw.ticket FROM  sys_wechatparameter sw WHERE  sw.id = '2'
    </select>
       
</mapper>
